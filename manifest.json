{
  "manifest_version": 2,

  "name": "Pretty Wiki",
  "version": "0.1.0",
  "description": "extension to customized wikipedia look for better readability",

  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": ["script.js"],
      "matches": ["https://en.wikipedia.org/*"]
    }
  ]
}
